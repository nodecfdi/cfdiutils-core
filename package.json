    "description": "Core of CfdiUtils",
    "main": "dist/cfdiutils-core.cjs.js",
    "module": "dist/cfdiutils-core.esm.js",
    "types": "dist/cfdiutils-core.d.ts",
    "files": [
        "dist"
    ],
    "homepage": "https://github.com/nodecfdi/cfdiutils-core",
