        "@types/node": "^17.0.18",
        "@types/temp": "^0.9.1",
        "@typescript-eslint/eslint-plugin": "^5.12.0",
        "@typescript-eslint/parser": "^5.12.0",
        "eslint": "^8.9.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "jest": "^27.5.1",
        "prettier": "^2.5.1",
        "rollup": "^2.67.3",
        "ts-jest": "^27.1.3",
        "ts-node": "^10.5.0",
        "tslib": "^2.3.1",
        "typedoc": "^0.22.11",
